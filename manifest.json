{
  "manifest_version": 2,
  "name": "Dazzle",
  "version": "1.01",

  "description": "",

  "permissions": ["webNavigation", "storage", "cookies", "<all_urls>", "tabs", "webRequest", "webRequestBlocking"],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "Dazzle.png",
    "96": "Dazzle.png"
  },
  "browser_action": {
    "default_title": "Diluvian Dazzle",
    "default_popup": "popup/popup.html",
    "default_icon": {
      "48": "Dazzle.png",
      "96": "Dazzle.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "all_frames": true
    }
  ]
}
